-- Create Semester 1 table
CREATE TABLE students_sem1 (
    student_id INT PRIMARY KEY,
    name VARCHAR(100),
    course VARCHAR(50)
);

-- Create Semester 2 table
CREATE TABLE students_sem2 (
    student_id INT PRIMARY KEY,
    name VARCHAR(100),
    course VARCHAR(50)
);

